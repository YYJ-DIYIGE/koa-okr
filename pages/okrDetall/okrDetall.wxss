
.okrCreate-obj{
  background-color:#C8C8C8;
  margin: 10rpx 30rpx;
  padding: 0 30rpx;
  /* height: 155rpx; */
  position: relative;
  overflow:hidden;
}
.okrCreate-obj-title{
  line-height: 30px;
  padding-top: 20rpx;
}
.okrCreate-obj-time{
  font-size: 12px;
  color: #333; 
  padding: 10rpx;
}
.okrCreate-obj.active:after{
  content: "完成";
  display: block;
  width: 120rpx;
  height: 120rpx;
  border: 2px solid #333;
  line-height: 120rpx;
  text-align: center;
  border-radius: 50%;
  position: absolute;
  top: -13px;
  right: -13px;
  -webkit-transform:rotate(100deg);
  transform:rotate(-50deg);
  color: #333;
}
.okrCreate-keyresult-item{
  margin: 10px;
  margin-top: 20px;
}
.okrCreate-item-title{
 font-size: 15px;
 margin-left: 20px;
 padding-left: 20px;
 display: flex;
 position: relative;
}
.okrCreate-item-title .sign{
  font-size: 10px;
  color: rgb(185, 51, 51);
  position: absolute;
  right: 10px;
  top: 3px;
}
.okrCreate-item-todo{
  display: inline-block;
  font-size: 12px;
  margin-top: 5px;
  padding: 2px 40px;
}
.okrCreate-item-title::before{
  content: '';
  display: block;
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  border: 1px solid rgb(139, 118, 118);
  position: absolute;
  top: 5px;
  left: -4px;
}
.okrCreate-item-title.active:before{
  content: '';
  display: block;
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  border: 1px solid rgb(139, 118, 118);
  background-color: rgb(139, 118, 118);
  position: absolute;
  top: 5px;
  left: -4px;
}
.okrCreate-item-todo.active::after{
  content: '';
  display: block;
  border: 2rpx solid rgb(189, 128, 128);
  margin-top: -7px;
}