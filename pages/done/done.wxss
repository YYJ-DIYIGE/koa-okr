.done-content{
  height: 100%;
}
.done-title{
  font-size: 14px;
  margin: 20rpx 0;
  color: #333;
}
.done-item{
  background-color: #f5f5f5;
  height: 170rpx;
  margin: 20rpx 0;
  position: relative;
  overflow:hidden;
}
.done-item-text{
  padding-left: 20rpx;
}
.done-item-okr{
  padding: 10rpx 0;
}
.done-item-time{
  line-height: 15px;
  font-size: 12px;
  margin-bottom: 10rpx;
  color: #333;
}
.done-item.active:after{
  content: "完成";
  display: block;
  width: 120rpx;
  height: 120rpx;
  border: 2px solid #333;
  line-height: 120rpx;
  text-align: center;
  border-radius: 50%;
  position: absolute;
  top: -10px;
  right: -9px;
  -webkit-transform:rotate(100deg);
  transform:rotate(-50deg);
  color: #333;
}